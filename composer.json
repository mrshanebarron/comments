{
    "name": "mrshanebarron/comments",
    "description": "Comments/discussion component for Laravel - supports Livewire and Vue",
    "type": "library",
    "license": "MIT",
    "authors": [{"name": "Shane Barron", "email": "mrshanebarron@gmail.com"}],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "mrshanebarron/likes": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "MrShaneBarron\\Comments\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": ["MrShaneBarron\\Comments\\CommentsServiceProvider"]
        }
    },
    "minimum-stability": "stable"
}
